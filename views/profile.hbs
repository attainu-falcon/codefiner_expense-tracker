{{#if msg}}
    <div id="alert-msg" class="alert alert-success text-center collapse">
       <a href="#" id="close-alert" class="close" data-dismiss="alert">&times;</a>
       <strong>{{msg}}</strong>
    </div>
 {{/if}} 

 {{#if error}}
    <div id="alert-msg" class="alert alert-danger text-center collapse">
       <a href="#" id="close-alert" class="close" data-dismiss="alert">&times;</a>
       <strong>{{error}}</strong>
    </div>
 {{/if}}